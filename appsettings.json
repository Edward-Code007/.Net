{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "tests.Controllers.AuthController": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "default": "Server=(localdb)\\mssqllocaldb;Database=Test;Trusted_Connection=True;"
  },
  "JwtOption": {
    "Issuer": "localDb",
    "Audience": "localDb",
    "SecretKey": "MySuperDuperSecretKeyForAutheticationFuckYeahMySuperDuperSecretKeyForAutheticationFuckYeahMySuperDuperSecretKeyForAutheticationFuckYeah"
  },
  "SecurityManners": {
    "EncryptKey": "1234567890101112",
    "InitVector": "1234567890101112"

  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "Aplication": "Candela"
    }
  }

}
